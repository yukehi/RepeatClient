thers is an bug wehn trying to clear the error the prevValue disapeer\n        setProfile(prevValue =>{\n            if(name === 'name'){\n                return {\n                    name:value,\n                    email:prevValue.email,\n                    password:prevValue.password,\n                    error:''\n                }\n            }else if(name === 'email'){\n                return {\n                    name:prevValue.name,\n                    email:value,\n                    password:prevValue.password,\n                    error:''\n                }\n            }else if(name === 'password'){\n                return {\n                    name:prevValue.name,\n                    email:prevValue.email,\n                    password:value,\n                    error:''\n\n                }\n        }\n    })\n}\n    const clickSubmit = (e) =>{\n        e.preventDefault()\n        const {name, email,password} = profile\n        const user = {\n            name,\n            email,\n            password,\n        };\n        fetch(`${process.env.REACT_APP_API_URL}/signup`, {\n            method: 'POST',\n            headers: {\n                'Accept': 'application/json',\n                'Content-Type': 'application/json'\n            },\n            body: JSON.stringify(user)\n        })\n        .then(response => {\n            return response.json()\n        })\n        .catch(err => console.log(err))\n        .then((data) => {\n            if (data.error){\n                setProfile({ error: data.error})\n            }else{\n                setProfile({name:'',email:'',password:'',error:'',open:true})\n            } \n            //need to move an error if data is with error\n            \n            }\n        )\n        \n    }\n    \n\n    return (<div className=\"container\">\n        <h2 className=\"mt-5 mb-5\">Sgin up</h2>\n        <h2 className=\"mt-5 mb-5\">Hello {profile.name}</h2>\n        <div className=\"alert alert-primary\" style={{display: profile.error ? '' : 'none'}}>{profile.error}</div>\n        <div className=\"alert alert-info\" style={{display: profile.open ? '' : 'none'}}>New account successfully created. please log in</div>\n        <form>\n            <div className=\"from-group\">\n                <label className=\"text-muted\">Name</label>\n                <input name=\"name\" type=\"text\" value={profile.name} onChange={handleChange} type=\"text\" className=\"form-control\"/>\n                <label  className=\"text-muted\">Email</label>\n                <input name=\"email\" type=\"email\" value={profile.email} onChange={handleChange} type=\"email\" className=\"form-control\"/>\n                <label className=\"text-muted\">password</label>\n                <input name=\"password\" type=\"password\" value={profile.password} onChange={handleChange} type=\"password\" className=\"form-control\"/>\n            </div>\n            <button onClick={clickSubmit} type=\"submit\" className=\"btn btn-raised btn-primary\">submit</button>\n        </form>\n    </div>)\n\n\n}\n\n\nexport default SginUp;","/Users/alekseyzgeria/OneDrive - yukehi/Web Development/React/socialapp/react-front/src/user/SginIn.jsx",["50","51"],"/Users/alekseyzgeria/OneDrive - yukehi/Web Development/React/socialapp/react-front/src/core/Menu.jsx",["52"],"/Users/alekseyzgeria/OneDrive - yukehi/Web Development/React/socialapp/react-front/src/user/Profile.jsx",[],"/Users/alekseyzgeria/OneDrive - yukehi/Web Development/React/socialapp/react-front/src/core/Loading.jsx",[],"/Users/alekseyzgeria/OneDrive - yukehi/Web Development/React/socialapp/react-front/src/core/OnHover.jsx",[],"/Users/alekseyzgeria/OneDrive - yukehi/Web Development/React/socialapp/react-front/src/auth/index.js",[],"/Users/alekseyzgeria/OneDrive - yukehi/Web Development/React/socialapp/react-front/src/App.js",[],{"ruleId":"53","severity":1,"message":"54","line":82,"column":93,"nodeType":"55","endLine":82,"endColumn":104},{"ruleId":"53","sev